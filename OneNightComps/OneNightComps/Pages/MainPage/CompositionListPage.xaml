<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:mainpage="clr-namespace:OneNightComps.Pages.MainPage"
             x:Class="OneNightComps.Pages.MainPage.CompositionListPage">
    <ContentPage.Content>
        <ListView x:Name="CompositionsListView" HasUnevenRows="true">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <mainpage:CompositionListCell/>
                </DataTemplate>
            </ListView.ItemTemplate>
            <ListView.Footer>
                <StackLayout/>
            </ListView.Footer>
        </ListView>
    </ContentPage.Content>
</ContentPage>